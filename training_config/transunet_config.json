{
    "model_type": "transunet",
    "num_epochs": 10,
    "batch_size": 2,
    "img_size": 512,
    "encoder_lr": 0.0001,
    "decoder_lr": 0.001,
    "weight_decay": 0.01,
    "gradient_clip": 1.0,
    "patience": 25,
    "optimizer": "adamw",
    "scheduler": "cosine_warm_restarts",
    "dropout": 0.1,
    "patch_size": 16,
    "embed_dim": 768,
    "num_heads": 12,
    "transformer_layers": 12,
    "loss_type": "cross_entropy",
    "label_smoothing": 0.1,
    "ce_weight": 0.1,
    "iou_weight": 0.4,
    "dice_weight": 0.5,
    "data_mode": "image_mask",
    "save_epoch_visualizations": true,
    "save_frequency": 10,
    "log_frequency": 20
} 